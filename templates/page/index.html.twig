{% extends 'main/default.html.twig' %}
{% block title %}Главная{% endblock %}

{% block body %}
<div class="example-wrapper">
    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
        <button class="green"><a href="{{ path('app_logout') }}">Выйти</a></button>
    {% else %}
        <button class="btn"><a href="{{ path('app_login') }}">Войти</a></button>
        <button class="btn"><a href="{{ path('app_register') }}">Регистрация</a></button>
    {% endif %}
</div>
{% endblock %}
